{
  "name": "socimeet",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc --project .",
    "server": "nodemon -r dotenv/config --verbose --watch \"server\" --ext \"ts,json\" --exec ts-node server/server.ts",
    "client": "cd client && npm start && cd ..",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:app": "node -r reflect-metadata -r dotenv/config -r ts-node/register server.ts",
    "start_loc": "node -r reflect-metadata -r dotenv/config -r ts-node/register server.ts",
    "start_pro": "npx ncp .env.pro .env && npm run start:app"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cfenv": "^1.0.4",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mongoose": "^6.3.0",
    "node-fetch": "^2.6.0",
    "ramda": "0.26.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "ts-node": "^8.7.0",
    "tslib": "^1.9.1",
    "yargs": "^11.0.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^10.17.4",
    "@types/node-fetch": "^2.5.3",
    "@types/ramda": "^0.26.43",
    "nodemon": "^2.0.15"
  }
}
