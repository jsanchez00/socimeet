    "yargs": "^11.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cp .env.loc .env && concurrently \"npm run client\" \"npm run server\"",
    "server": "nodemon -r dotenv/config --verbose --watch \"server\" --ext \"ts,json\" --exec ts-node server/server.ts",
    "client": "cd client && npm start && cd ..",
    "prebuild-prod": "rm -rf dist",
    "build-prod": "cp .env.pro .env && yarn install && cd client && yarn install && npm run build && cd .. && yarn install && npm run build",
    "build": "tsc --project . && npm run cp:client",
    "cp:client": "npx mkdirp dist/client/build && npx ncp client/build dist/client/build && npx ncp package.json dist",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "yarn install && node -r dotenv/config server.js",
    "update:deps": "rm -rf node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsanchez00/socimeet.git"
  },
