    "@types/node": "^17.0.24",
    "@types/node-fetch": "^2.5.3",
    "@types/ramda": "^0.26.43",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cfenv": "^1.0.4",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "express": "^4.17.3",
    "guid-typescript": "^1.0.9",
    "mongoose": "^6.3.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "ramda": "0.26.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "styled-components": "^5.3.5",
    "ts-node": "^10.7.0",
    "tslib": "^1.9.1",
    "tslint": "^6.1.3",
